class A {
    greet(){
        print "hello from A";
    }
}


class B(A){ // we should also have access to this syntax ```class B(A){}```
    greet(){
        print "hello from B";
        super(A).greet();
    }
}

class C(A){
    greet(){
        print "hello from C";
        super(A).greet();
    }
}

class D(C){
    greet(){
        print "hello from D";
        super(C).greet();
    }
}
var b = B();
b.greet();
