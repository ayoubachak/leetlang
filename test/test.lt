
class A {
    greet(){
        print "hello from A";
    }
}


class B < A{ // we should also have access to this syntax ```class B(A){}```
    greet(){
        print "hello from B";
        super.greet();
    }
}

class C < A{
    greet(){
        print "hello from C";
        super.greet();
    }
}

class D < C{
    greet(){
        print "hello from D";
        super.greet();
    }
}
var b = B();
b.greet();

print("Available types");
print("type(true)=" + type(true));
print("type(1)=" + type(1));
print("type('hello')=" + type("hello"));
print("type([1])=" + type([1]));
print("isinstance(b, B)=" + isinstance(b, B));

var numbers = [1, 2, 3, 4, 5, 6, 7];
print(numbers[1]);
numbers[1] = 1;
print(numbers[1]); 
