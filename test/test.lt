class A {
    greet(){
        print "hello from A";
    }
}


class B < A{ // we should also have access to this syntax ```class B(A){}```
    greet(){
        print "hello from B";
        super.greet();
    }
}

class C < A{
    greet(){
        print "hello from C";
        super.greet();
    }
}

class D < C{
    greet(){
        print "hello from D";
        super.greet();
    }
}
var b = B();
b.greet();
